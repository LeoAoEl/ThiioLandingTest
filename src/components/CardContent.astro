---
import Container from "@components/Container.astro";
import LeftCard from "@components/common/LeftCard.astro";
import RightCard from "@components/common/RightCard.astro";
---

<Container extraClases=" grid grid-cols-1 md:grid-cols-2 my-16">
  <div
    class="col-span-1 mx-auto flex place-content-center items-center justify-center"
  >
    <LeftCard />
  </div>
  <div class="col-span-1 mx-auto">
    <RightCard />
  </div>
</Container>
